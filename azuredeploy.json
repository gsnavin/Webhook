{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "docusignDeveloperEmail": {
      "type": "string"
    },
    "docusignDeveloperPassword": {
      "type": "securestring"
    },
    "docusignIntegratorKey": {
      "type": "securestring"
    }
  },
  "resources": [
    {
      "resources": [
        {
          "apiVersion": "2014-04-01",
          "type": "config",
          "name": "web",
          "properties": {
            "appSettings": [
              {
                "name": "docusignDeveloperEmail",
                "value": "[parameters('docusignDeveloperEmail')]"
              },
              {
                "name": "docusignPassword",
                "value": "[parameters('docusignPassword')]"
              },
              {
                "name": "docusignIntegratorKey",
                "value": "[parameters('docusignIntegratorKey')]"
              }
            ]
          }
        }
      ]
    }
  ]
}
